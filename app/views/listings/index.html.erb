<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Location</th>
      <th>Price</th>
      <th>Breed</th>
      <th>Sex</th>
      <th>Animal type</th>
      <th>Microchip number</th>
      <th>Desexed status</th>
      <th>Expected size</th>
      <th>Vaccination status</th>
      <th>Health status</th>
      <th>Phone number</th>
      <th>Email</th>
      <th>Description</th>
      <th>Vaccination details</th>
      <th>Organisations profile</th>
      <th>Users profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.name %></td>
        <td><%= listing.age %></td>
        <td><%= listing.location %></td>
        <td><%= listing.price %></td>
        <td><%= listing.breed %></td>
        <td><%= listing.sex %></td>
        <td><%= listing.animal_type %></td>
        <td><%= listing.microchip_number %></td>
        <td><%= listing.desexed_status %></td>
        <td><%= listing.expected_size %></td>
        <td><%= listing.vaccination_status %></td>
        <td><%= listing.health_status %></td>
        <td><%= listing.phone_number %></td>
        <td><%= listing.email %></td>
        <td><%= listing.description %></td>
        <td><%= listing.vaccination_details %></td>
        <td><%= listing.organisations_profile_id %></td>
        <td><%= listing.users_profile_id %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
